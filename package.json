{
  "name": "toolkit.dev",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build-pure": "next build",
    "build": "dotenv -e .env.local -- prisma migrate deploy && next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate": "dotenv -e .env.local -- prisma migrate deploy",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset",
    "dev": "pnpm dlx tsx setup/index.ts && next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "ci": "pnpm run format:check && pnpm run check && pnpm run build-pure",
    "shiki:generate": "pnpm dlx tsx src/components/ui/code/codegen.ts && pnpm format:write"
  },
